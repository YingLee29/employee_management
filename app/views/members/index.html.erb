<div class="d-flex justify-content-center">
  <div class="">
    <%= link_to "Create User", new_member_path, class:"btn btn-primary" %>
  </div>
  <div class="mt-5 user-list-container">
    <% @users.each_with_index do |user, index| %>
      <div>
        <%= "#{index + 1}. #{user.email}" %>
      </div>
    <% end %>
  </div>
</div>
