<div class="d-flex justify-content-center">
  <div class="">
    <%= link_to "Create Project", new_project_path, class:"btn btn-primary" %>
  </div>
  <div class="mt-5 user-list-container">
    <% @projects.each_with_index do |project, index| %>
      <div>
        <%= "#{index + 1}. #{project.name}" %>
      </div>
    <% end %>
  </div>
</div>
